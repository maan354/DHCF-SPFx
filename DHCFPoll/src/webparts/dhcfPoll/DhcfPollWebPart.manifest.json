{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx/client-side-web-part-manifest.schema.json",
  "id": "7c283be6-e568-4932-9349-b1a2e28e8a75",
  "alias": "DhcfPollWebPart",
  "componentType": "WebPart",

  // The "*" signifies that the version should be taken from the package.json
  "version": "*",
  "manifestVersion": 2,

  // If true, the component can only be installed on sites where Custom Script is allowed.
  // Components that allow authors to embed arbitrary script code should set this to true.
  // https://support.office.com/en-us/article/Turn-scripting-capabilities-on-or-off-1f2c515f-5d7e-448a-9fd7-835da935584f
  "requiresCustomScript": false,

  "preconfiguredEntries": [{
    "groupId": "4aca9e90-eff5-4fa1-bac7-728f5f157b66",
    "group": { "default": "Tools" },
    "title": { "default": "Simple Poll" },
    "description": { "default": "Insert a simple poll (one question) based on a SharePoint survey list. This Web Part is perfect for simple surveys to insert on a homepage for example. Users can vote in 1 click and see the results in the form of a pie or bar chart." },
    "officeFabricIconFontName": "CustomList",
    "properties": {
      "chartType": "pie",
      "forceVoteToViewResults": false,
      "size": "24px",
      "colo": "#323232"
    }
  }]
}
